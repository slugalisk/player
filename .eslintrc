{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "import",
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-unused-vars": ["error", {"args": "none"}],
    "no-console": ["error", {"allow": ["log", "warn", "error"]}]
  }
}
